{
  "name": "koa-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "TS_NODE_BASEURL=dist NODE_ENV=unittest,strict nyc node --preserve-symlinks node_modules/.bin/_mocha -r tsconfig-paths/register -t 10000000 $(find dist -name \\*.test.js)",
    "test:rest": "NODE_ENV=resttest,strict node --preserve-symlinks node_modules/.bin/_mocha -t 10000000 $(find dist/test -name \\*.test.js)",
    "link-fe": "rm -f ./fe && ln -s ../fe/build ./fe",
    "build": "npm run clean && npm run buildts && npm run lint && npm run link-fe",
    "build:prod": "npm run clean && npm run buildts:prod && npm run lint && npm run link-fe",
    "lint": "eslint -c .eslintrc.js --ext ts src",
    "buildts": "node node_modules/.bin/tsc -p tsconfig.json",
    "buildts:prod": "node node_modules/.bin/tsc -p tsconfig.prod.json",
    "clean": "rm -rf dist",
    "postinstall": "script/postinstall.sh",
    "node": "node",
    "start:prod:prof": "TS_NODE_BASEURL=dist NODE_ENV=production node -r tsconfig-paths/register --prof --nolazy --preserve-symlinks dist/index.js",
    "start:prod": "TS_NODE_BASEURL=dist NODE_ENV=production node -r tsconfig-paths/register --nolazy --preserve-symlinks dist/index.js",
    "start:prof": "TS_NODE_BASEURL=dist NODE_ENV=development,no node -r tsconfig-paths/register --prof --nolazy --preserve-symlinks dist/index.js",
    "start": "TS_NODE_BASEURL=dist NODE_ENV=development node -r tsconfig-paths/register --nolazy --preserve-symlinks dist/index.js"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@koa/router": "^12.0.1",
    "cls-hooked": "^4.2.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^5.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node": "^20.10.0",
    "passport-saml": "^3.2.1",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/cls-hooked": "^4.3.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.13.12",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-json": "^2.0.23",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-session": "^6.4.5",
    "@types/koa-static": "^4.0.4",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "bufferutil": "^4.0.8",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "utf-8-validate": "^6.0.3"
  }
}
